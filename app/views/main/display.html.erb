                <h2>DISASTER INCIDENT WORKSHEET</h2>
            </div>

            <!-- Example row of columns -->
            <div class="row">
                <div class="span6 offset3">
                    <form id="main_form" action="/main/new">
                        <p>Incident Responder:</p>
                        <input class="span5" name="dispatcher" placeholder="DISPATCHER:">
                        <input class="span5" name="initial_responder" placeholder="Initial Responder">
                        
                        <input class="span5" name="dispatched_date" placeholder="Dispatched Date">
                        <input class="span5" name="date_on_scene" placeholder="Date on scene">
                        <input class="span5" name="time_on_scene" placeholder="Time on scene">
                        <input class="span5" name="date_left_scene" placeholder="Date left scene">
                        <input class="span5" name="time_left_scene" placeholder="Date left scene">

                        <p>Client Info:</p>
                        <input class="span5 client" placeholder="salutation" name="salutation">
                        <input class="span5 client" placeholder="first_name" name="first_name">
                        <input class="span5 client" placeholder="middle_name" name="middle_name">
                        <input class="span5 client" placeholder="last_name" name="last_name">
                        <input class="span5 client" placeholder="suffix" name="suffix">
                        <input class="span5 client" placeholder="home_address" name="home_address">
