                        <input class="span5 client" placeholder="state" name="state">
                        <input class="span5 client" placeholder="county" name="county">
                        <input class="span5 client" placeholder="country" name="country">
                        <input class="span5 client" placeholder="home_phone_nbr" name="home_phone_nbr">
                        <input class="span5 client" placeholder="work_phone_nbr" name="work_phone_nbr">
                        <input class="span5 client" placeholder="mobile_phone_nbr" name="mobile_phone_nbr">
                        <input class="span5 client" placeholder="date_of_birth" name="date_of_birth">
                        <input class="span5 client" placeholder="date_deceased" name="date_deceased">
                        <input class="span5 client" placeholder="head_of_household" name="head_of_household">
                        <input class="span5 client" placeholder="ethnicitytypeid" name="ethnicitytypeid">
                        <input class="span5 client" placeholder="language" name="language">
                        <!-- <button id="add_member btn"><i class="icon-plus-sign"></i>Save and add new Family member</button> -->

                        <input class="span5" name="disaster_type" placeholder="TYPE OF DISASTER:">
                        <input class="span5" name="case_id" placeholder="CAS INCIDENT ID:">
                        <input class="span5" name="dispatcher_num" placeholder="DISASTER #:">
                        <input class="span5" name="dispatch_time" placeholder="DISPATCHED TIME:">
                        <input class="span5" name="arc_person" placeholder="ARC PERSON RECEIVING DISPATCH CALLTIME RESPONDERS ON SCENE:">
                        <input class="span5" name="time_left" placeholder="TIME RESPONDERS LEFT SCENE:">
                        <textarea class="span5" name="all_personel_responding" placeholder="NAMES AND PHONE NUMBERS ALL PERSONNEL RESPONDING(Mass Care, Health Services, DMH, etc.):"></textarea>
                        <input class="span5" name="" placeholder="SEQUENCEOF EVENTS">
                        <input class="span5" name"dwelling_type" placehoder="TYPE OF DWELLING:">
                        <textarea class="span5" name="damage_assesment" placeholder="DAMAGE ASSESSMENT(broken down by room/unit):"></textarea>
                        <input class="span5" name="adults_serviced" placeholder="NUMBER OF TOTAL ADULTS SERVICED:">
                        <input class="span5" name="children_serviced" placeholder="NUMBER OFTOTAL CHILDREN SERVICED:">
                        <textarea class="span5" name="injuries_fatalities" placeholder="ANY INJURIES OR DEATHS? (explain)LIST OF TENANTS (HEAD OF HOUSEHOLD ONLY)  / PHONE #/UNITS AFFECTED/LANDLORD INFO (if available):"></textarea>
                        <input class="span5" name="unserviced_clients" placeholder="UNSERVICED CLIENTS/UNITS AND WHYCAC # AND AMOUNTS PROVIDED:">
                        <input class="span5" name="stock_provided" placeholder="STOCK PROVIDED (Comfort Kits/PrePacks/Teddy Bears):">
                        <input class="span5" name="case_number_901" placeholder="CASE NUMBER FROM 901(s):">
                        <input class="span5" name="services" placeholder="MASS CARE SERVICES?HEALTH SERVICES?MENTAL HEALTH SERVICES?PARTNER SERVICES?DISASTER SERVICES TECHNOLOGY?">
                        <textarea class="span5" name="additional_info" placeholder="ANY ADDITIONAL INFO:"></textarea>
                        <input class="span5" name="total_cost" placeholder="Total cost">
                        <input class="span5" name="client_language" placeholder="Language of client">

                        <p>***FOR OFFICE USE ONLY***</p>
                        <p>TRAINEE FOLLOW UP</p>
                        <input name="date_followed_up" placeholder="DATE FOLLOWED UP">

                        <input class="span5" name="" placeholder="NAME OF PERSON DOING FOLLOW UP:">
                        <input class="span5" name="" placeholder="RESULT OF FOLLOW UP:">
                        <input class="span5" name="" placeholder="COORDINATOR CONFIRMED:">
                        <div class="form-actions">
                        <input class="btn span3 btn-primary" type="submit" id="submit" placeholder="submit">
                       <!--  <button type="submit" class="btn btn-primary">Save changes</button> -->
                        <button type="button" class="btn btn-danger">Cancel</button>
